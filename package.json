{
    "name": "map-replace-js",
    "displayName": "map-replace.js",
    "description": "Replace selected string with custom JavaScript function",
    "version": "0.0.1",
    "publisher": "Yukai",
    "engines": {
        "vscode": "^1.19.0"
    },
    "categories": [
      "Formatters", "Other"
    ],
    "activationEvents": [
        "onCommand:extension.mapSelectionWithJsFunc"
    ],
    "icon": "doc/images/icon-128.png",
    "galleryBanner": {
        "color": "#1c1f27",
        "theme": "dark"
    },
    "keywords": [
      "javascript",
      "map",
      "function",
      "replace",
      "edits"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.mapSelectionWithJsFunc",
                "title": "Map and Replace Selections with JavaScript Function"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
